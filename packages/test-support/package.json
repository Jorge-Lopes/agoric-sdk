{
  "name": "@agoric/test-support",
  "version": "0.1.0",
  "description": "Support libraries for testing on chain",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "test:xs": "exit 0",
